package Data;

/**
 * Handle Data Base
 * @author DongKyu
 *
 */
public class DBProvider {
	private static DBProvider singleton = null;

	
	private DBProvider() {
	}

	public static DBProvider getInstance()
	{ 
		if(singleton == null)
		{
			singleton  = new DBProvider();
		}
		return singleton;
	}
}
